{
    "extends": "./node_modules/gts/tsconfig-google.json",
    "compileOnSave": false,
    "compilerOptions": {
        "resolveJsonModule": true,
        "moduleResolution": "node",
        "strictNullChecks": true,
        "removeComments": true,
        "allowJs": false,
        "rootDir": "src",
        "strict": true,
        "outDir": "build",
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "lib": ["es2021"],
    },
    "include": ["src/**/*.ts"]
}
